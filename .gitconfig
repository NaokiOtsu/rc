[alias]
  co = checkout
  st = status
  tree = log --graph --abbrev-commit --decorate --date=short --pretty=format:'%Cgreen%h %cd %Cblue%cn %Creset%s'
  graph = log --graph --date-order -C -M --pretty=format:\"<%h> %ad [%an] %Cgreen%d%Creset %s\" --all --date=short

[core]
    excludesfile = .gitignore_global

[github]
    user = zentooo

[include]
    path = .gitconfig.local
